{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        [
            "env",
            {
                "targets":{
                    "browsers":["last 2 versions", "safari >= 7"]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator":true
            }
        ]
    ]
}
    